{
  "name": "@item-enonic-types/lib-sentry",
  "version": "0.0.2",
  "description": "Type definitions for lib-sentry",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:server": "webpack --config webpack.config.js --color",
    "build": "run-p -c build:*",
    "dist:server": "cross-env NODE_ENV=production webpack --config webpack.config.js --color",
    "dist": "run-p -c dist:*",
    "lint": "eslint --fix 'src/**/*.ts'",
    "prepublishOnly": "tsc --declarationDir build/types --emitDeclarationOnly true --declaration true && cp -r ./build/types/* .",
    "postpublish": "rm ./*.d.ts && rm -r ./format"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/register": "^7.21.0",
    "@enonic-types/lib-admin": "^7.12.2",
    "@item-enonic-types/global": "^7.12.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-loader": "^9.1.2",
    "browserslist-config-enonic": "^1.0.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "glob": "^10.2.1",
    "html-webpack-plugin": "^5.5.1",
    "prettier": "^2.8.7",
    "ramda": "^0.29.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.1"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.10.0"
  }
}
